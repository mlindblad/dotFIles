[user]
	name = Martin Lindblad
	email = martin.lindblad@find-out.se
[credential]
	helper = osxkeychain
[core]
	editor = subl -n -w
	excludesfile = /Users/martin/.gitignore
[alias]
	undo-last-commit = reset --soft @~1
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
